{
    "interface_name": "ai.clea.examples.BLEDevices",
    "version_major": 1,
    "version_minor": 0,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "description": "Set of scanned devices with corresponding presence time in a time span",
    "mappings": [
        {
            "endpoint" : "/bleDevices/devices",
            "type" : "stringarray",
            "database_retention_policy" : "use_ttl",
            "database_retention_ttl" : 31536000,
            "description" : "List of MAC addresses detected in the current time span"
        },
        {
            "endpoint" : "/bleDevices/presence_time",
            "type" : "integerarray",
            "database_retention_policy" : "use_ttl",
            "database_retention_ttl" : 31536000,
            "description" : "List of presence time in milliseconds related to devices listed in previous mapping"
        }
    ]
  }