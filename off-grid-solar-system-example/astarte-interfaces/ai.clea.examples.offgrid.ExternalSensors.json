{
    "interface_name": "ai.clea.examples.offgrid.ExternalSensors",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "mappings": [
        {
            "endpoint": "/%{sensorId}/sensorValue",
            "type": "double",
            "reliability":"unique",
            "retention":"volatile",
            "expiry":60,
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000
        },
        {
            "endpoint": "/%{sensorId}/sensorType",
            "type": "string",
            "reliability":"unique",
            "retention":"volatile",
            "expiry":60,
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000
        },
        {
            "endpoint": "/%{sensorId}/sensorDisplayString",
            "type": "string",
            "reliability":"unique",
            "retention":"volatile",
            "expiry":60,
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 31536000
        }
    ]
}
