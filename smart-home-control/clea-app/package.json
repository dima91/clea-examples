{
  "name": "clea-home-demo",
  "version": "0.0.1",
  "description": "Clea Home App displays current status of devices in home and sends command to home.",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --mode=development --watch",
    "extract": "formatjs extract 'src/**/*.ts*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile-folder lang src/lang",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "Rajesh Reddy Nanjarapalli",
  "private": true,
  "license": "Apache-2.0",
  "dependencies": {
    "@types/phoenix": "^1.5.4",
    "apexcharts": "^3.26.3",
    "axios": "^0.21.1",
    "babel-plugin-formatjs": "^10.3.0",
    "bootstrap": "^5.1.3",
    "dayjs": "^1.10.7",
    "phoenix": "^1.6.6",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-bootstrap": "^2.0.2",
    "react-customizable-progressbar": "^1.2.0",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@formatjs/cli": "^4.2.20",
    "@svgr/webpack": "^5.5.0",
    "@types/react-dom": "^17.0.8",
    "css-loader": "^5.2.4",
    "prettier": "2.3.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "formatjs",
        {
          "idInterpolationPattern": "[sha512:contenthash:base64:6]",
          "ast": true
        }
      ]
    ]
  }
}
