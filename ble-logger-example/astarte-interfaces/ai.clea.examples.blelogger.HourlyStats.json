{
    "interface_name": "ai.clea.examples.blelogger.HourlyStats",
    "version_major": 0,
    "version_minor": 1,
    "type": "datastream",
    "ownership": "device",
    "aggregation": "object",
    "mappings": [
        {
            "endpoint": "/accessoriesVendors",
            "type": "string",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 63072000,
            "explicit_timestamp": true,
            "description": "JSON object containing vendor names as keys and a list of detected accessories in a certain hour (static MAC addresses) as values."
        },
        {
            "endpoint": "/detectedAccessories",
            "type": "stringarray",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 63072000,
            "explicit_timestamp": true,
            "description": "Accessories (devices with static MAC address) detected in a certain hour."
        },
        {
            "endpoint": "/detectedSmartphones",
            "type": "stringarray",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 63072000,
            "explicit_timestamp": true,
            "description": "Smartphones (devices with random MAC address) detected in a certain hour."
        },
        {
            "endpoint": "/interactions",
            "type": "doublearray",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 63072000,
            "explicit_timestamp": true,
            "description": "Interactions performed in a certain hour with the target, measured in seconds."
        },
        {
            "endpoint": "/smartphonesVendors",
            "type": "string",
            "database_retention_policy": "use_ttl",
            "database_retention_ttl": 63072000,
            "explicit_timestamp": true,
            "description": "JSON object containing vendor names as keys and a list of detected smartphones in a certain hour (random MAC addresses) as values."
        }
    ]
}